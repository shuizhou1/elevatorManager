export default {
    namespaced: true,
    state: {
        id: 0,
        name: '',
        nickName: '',
        communityId: '',
        areacode:''
    },
    mutations: {
        updateId(state, id) {
            state.id = id
        },
        updateName(state, name) {
            state.name = name
        },
        updateNickName(state, nickName) {
            state.nickName = nickName
        },
        updateCommunityId (state, id) {
            state.communityId = id
        },
        updateAreacode(state, areacode) {
            state.areacode = areacode
        }
    }
}
