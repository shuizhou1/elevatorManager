.el-button {
    font-size: 14px;
    height: 44px;
    &.is-disabled {
        opacity: .6 !important;
    }
    .icon-edit, .icon-back {
        width: 16px;
        height: 16px;
        vertical-align: sub;
    }
    .icon-edit {
        background: url("~@/assets/img/icon-edit.png");
    }
    .icon-back {
        width: 17px;
        height: 17px;
        background: url("~@/assets/img/icon-back.png");
    }
    .icon-query {
        width: 13px;
        height: 14px;
        background: url("~@/assets/img/icon-query.png");
    }
    .icon-new {
        width: 14px;
        height: 15px;
        background: url("~@/assets/img/icon-new.png");
    }
    .icon-dele {
        width: 15px;
        height: 15px;
        background: url("~@/assets/img/icon-dele.png");
    }
    .icon-import {
        width: 16px;
        height: 16px;
        background: url("~@/assets/img/icon-import.png");
    }
    .icon-sign {
        width: 21px;
        height: 17px;
        background: url("~@/assets/img/icon-sign.png");
    }
}
.el-button--primary {
    background-color: $primary !important;
    border-color: $primary !important;
    &:focus, &:hover {
        background: rgba(30, 30, 160, .9) !important;
        border-color: rgba(30, 30, 160, .9) !important;
    }
}
.el-button--wraning {
    background-color: $warning !important;
    border-color: $warning !important;

    &:focus,
    &:hover {
        background: rgba(225, 158, 59, .9) !important;
        border-color: rgba(225, 158, 59, .9) !important;
    }
}

.el-button--danger {
    background-color: $danger !important;
    border-color: $danger !important;
    // color: $white !important;
    &:focus,
    &:hover {
        background: rgba(244, 108, 108, .9) !important;
        border-color: rgba(244, 108, 108, .9) !important;
        // color: $white !important;
    }
}
.el-button--back {
    background-color: $success !important;
    border-color: $success !important;
    color: $white !important;
    &:focus, &:hover {
        background: rgba(67, 178, 54, .9) !important;
        border-color: rgba(67, 178, 54, .9) !important;
        color: $white !important;
    }
}
.el-button--save {
    background-color: #91c384 !important;
    border-color: #91c384 !important;
    color: $white !important;
    &:focus, &:hover {
        background: rgba(145,195,132, .9) !important;
        border-color: rgba(145,195,132, .9) !important;
        color: $white !important;
    }
}
 .el-button--import {
     background-color: #d26f83 !important;
     border-color: #d26f83 !important;
     color: $white !important;
     &:focus, &:hover {
         background: rgba(210,111,131, .9) !important;
         border-color: rgba(210,111,131, .9) !important;
         color: $white !important;
     }
 }