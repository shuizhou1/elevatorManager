*{
  margin: 0;
  padding: 0;
}
html,body{
  position: relative;
  width: 100%;
  height: 100%;
  color: #555;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.clearfix:after{
  content: '';
  display: block;
  clear: both;
}
.el-dialog__wrapper{
  position: absolute;
  margin: 0 20px 20px 20px;
  top:50px;
}
